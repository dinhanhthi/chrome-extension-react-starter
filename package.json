{
  "name": "chrome-extension-react-starter",
  "version": "0.0.1",
  "description": "A starter for quickly creating a Chrome Extension that uses React and Tailwind CSS by default.",
  "main": "index.js",
  "license": "MIT",
  "author": "Anh-Thi Dinh",
  "repository": {
    "type": "git",
    "url": "https://github.com/IdetaAutomation/lnkd-extension"
  },
  "scripts": {
    "watch": "rm -rf dist/* && webpack --watch --progress --config webpack.config.js --mode=development",
    "build": "sh ./src/build_prod.sh",
    "build:prod": "rm -rf dist-prod/* && webpack --progress --config webpack.config.js --mode=production",
    "prettier": "prettier --write \"src/**/*.{ts,js,tsx,html}\"",
    "lint": "eslint --ext .ts,.tsx,.js,.jsx src",
    "playground": "vite"
  },
  "dependencies": {
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.8.0"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.5.1",
    "@trivago/prettier-plugin-sort-imports": "^4.1.1",
    "@types/chrome": "^0.0.188",
    "@types/lodash": "^4.14.191",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/webextension-polyfill": "^0.10.0",
    "@typescript-eslint/parser": "^5.54.1",
    "@vitejs/plugin-react": "^3.1.0",
    "classnames": "^2.3.2",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.7.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-tailwindcss": "^3.11.0",
    "html-webpack-plugin": "^5.5.0",
    "lodash": "^4.17.21",
    "postcss-loader": "^7.0.0",
    "prettier": "^2.8.4",
    "react-router-dom": "^6.9.0",
    "sass": "^1.59.2",
    "sass-loader": "^13.2.0",
    "style-loader": "^3.3.1",
    "tailwindcss": "3.3.1",
    "ts-loader": "^9.3.0",
    "typescript": "^4.6.4",
    "vite": "^4.2.0",
    "webextension-polyfill": "^0.10.0",
    "webpack": "^5.72.1",
    "webpack-cli": "^4.9.2",
    "webpack-merge": "^5.8.0"
  }
}
